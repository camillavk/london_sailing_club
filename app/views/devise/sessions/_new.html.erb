</br>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="form-group">
    <%= f.email_field :email, placeholder: "Enter email address", class: "form-control", autofocus: true %>
  </div>

  <div class="form-group">
    <%= f.password_field :password, placeholder: "Enter password", class: "form-control",  autocomplete: "off" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="form-group remember_me_login">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

  <div class="actions center-block">
    <%= f.submit "Login" %>
  </div>
<% end %>
